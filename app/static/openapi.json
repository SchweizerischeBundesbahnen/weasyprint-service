{
  "openapi": "3.0.3",
  "info": {
    "title": "WeasyPrint Service API",
    "version": "1.0.0"
  },
  "paths": {
    "/api/docs/dist/{filename}": {},
    "/api/docs/{path}": {},
    "/convert/html": {},
    "/static/openapi.json": {},
    "/version": {
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/VersionSchema"
                }
              }
            },
            "description": "Version data"
          }
        },
        "summary": "Get version info"
      }
    }
  },
  "components": {
    "schemas": {
      "VersionSchema": {
        "properties": {
          "chromium": {
            "description": "Chromium version",
            "type": "string"
          },
          "python": {
            "description": "Python version",
            "type": "string"
          },
          "timestamp": {
            "description": "Build timestamp",
            "type": "string"
          },
          "weasyprint": {
            "description": "WeasyPrint version",
            "type": "string"
          },
          "weasyprintService": {
            "description": "Service version",
            "type": "string"
          }
        },
        "required": [
          "python",
          "weasyprint"
        ],
        "type": "object"
      }
    }
  }
}
